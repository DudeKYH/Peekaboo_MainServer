{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "distributor:dev": {},
    "lobby:dev": {
      "dependsOn": ["distributor:dev"],
      "outputs": ["dist/**"]
    },
    "account:dev": {
      "dependsOn": ["lobby:dev"],
      "outputs": ["dist/**"]
    },
    "gateway:dev": {
      "dependsOn": ["account:dev"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  },
  "globalDependencies": [".env", "package.json"]
}
