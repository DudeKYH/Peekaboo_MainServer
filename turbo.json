{
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "distributor:build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "gateway:build": {
      "dependsOn": ["distributor:build"],
      "outputs": ["dist/**"]
    },
    "lobby:build": {
      "dependsOn": ["gateway:build"],
      "outputs": ["dist/**"]
    },
    "account:build": {
      "dependsOn": ["lobby:build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [".env"]
}
